	}	
	
	
	 /** start style navitagion style**/
	 
	#page .header .footer .body #sidebar ul.navigation{
	  margin:0 0  50px 8px;
	  padding:5px 0 0 0;
	  list-style-type:none; 
	  background-image:url('../images/border-dashed.gif');  
	  background-repeat:repeat-x;
	  background-position:top;
